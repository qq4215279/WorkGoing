<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gobestsoft.mapper.MMemberTransferMapper">

    <update id="updateAuid" parameterType="java.util.Map">
        UPDATE
              dept_member_transfer
        SET auid =  #{auid}
        WHERE member_id = #{member_id};

    </update>

</mapper>